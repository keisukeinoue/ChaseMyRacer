<h1>Listing course_points</h1>

<table>
  <tr>
    <th>Race</th>
    <th>Sequence</th>
    <th>Latitude</th>
    <th>Longitude</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @course_points.each do |course_point| %>
  <tr>
    <td><%= course_point.race_id %></td>
    <td><%= course_point.sequence %></td>
    <td><%= course_point.latitude %></td>
    <td><%= course_point.longitude %></td>
    <td><%= link_to 'Show', course_point %></td>
    <td><%= link_to 'Edit', edit_course_point_path(course_point) %></td>
    <td><%= link_to 'Destroy', course_point, confirm: 'Are you sure?', method: :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Course point', new_course_point_path %>
